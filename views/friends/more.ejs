<div class="row">
<title>More about your friend</title>
	<div class="col s6">
		<h3>More About <%=friend.friendname%>:</h3>
			<ul>
		 		<li>Event: <%=friend.event%></li>
		 		<li>Date: <%=friend.date%></li>
		 		<li>Tags: 
				 	<% friend.tags.forEach(function(t){ %>
							<span class="tags">#<%= t.content %></span>
					<% }); %>	
				</li>
		 	</ul>
	</div>
</div>

<div class="row">
	<% results.forEach(function(t){ %>
	<div class="col s12">
			<h4><%=t.tag.content%></h4>
				<% t.gifts.forEach(function(result) {%>
				<div class="col s12 m4 l2">
				  <div class="card customGift">
			        <div class="card-image">
			         	<img src="<%=result.Images[0].url_570xN%>">
						<a class="btn-floating halfway-fab waves-effect waves-light red" href="<%=result.url%>"><i class="material-icons">add</i></a>
				    </div>
				   	<div class="card-content">
				       	<h5><%=result.title.slice(0,30)+"..."%></h5>
				    </div>
				 </div>
		<!-- 			 <div class="col s12 center">
						<button class="next z-depth-0 btn white btn-flat center"><i class="fa fa-chevron-left fa-lg grey-text text-lighten-3"></i></button>
						<button class="next z-depth-0 btn white btn-flat center"><i class="fa fa-chevron-right fa-lg grey-text text-lighten-1"></i></button>
					</div> -->
			   </div>
			<% })%>
		   <hr>
		</div>
	<% }) %>
</div>