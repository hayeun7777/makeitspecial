<div class="row">
	<h1>Unique Gift Ideas</h1>
	  <div class="input-field col s12">
	  	<form method="GET" action="/product">
		    <select name="giftSearch">
		      <option value="" disabled selected>Choose Gifting Category</option>
		      <option value="birthday-gifts">Birthday</option>
		      <option value="wedding-gifts">Wedding</option>
		      <option value="valentines-day">Valentine's Day</option>
		    </select>
		     <button class="btn waves-effect waves-light" type="submit" name="action">Go Find Cool Stuff!
			  <i class="material-icons right">thumb_up</i>
			 </button>
		</form>
	  </div>

	<div class="col 12">
		<div class="row">
			<% gifts.forEach(function(gift){ %>
				<div class="col m4 s12 giftList">
					<div class="fixed-height">
				  	  <h5><b><%=gift.item%></b></h5>
				  	 </div>
				  	 <a href="http://www.uncommongoods.com<%=gift.link%>"><img src="http://www.uncommongoods.com<%=gift.photo%>"></a>
				    <h5>$<%=gift.getPrice()%> <%=gift.priceCurrency%></h5>
				    <a href="http://www.uncommongoods.com<%=gift.link%>"><h5>Click for more details</h5></a>
				</div>
			<% }) %>
		</div>
	</div>
</div>

<script>
$(document).ready(function(){
    $('select').formSelect();
});
        
</script>